import java.util.Arrays;
public class Samecharacterset{
    public static boolean areSamecharacterset(string str1 , string str2)
    str1 = str1.replaceAll(" " , "").toLowerCase();
    str2 = str2.replaceAll(" " , "").toLowerCase();
    
    char[] charArray1 = str1.toCharArray();
    char[] charArray2 = str2.toCharArray();
    Array.sort(charArray1);
    Array.sort(charArray2);
    
    return Arrays.equals(charArray1, charArray2);
}
public  static void main(String[] args)
String str1 = "team";
String str2 = "meat";
if(areSamecharacterset(str1,str2);
    System.out.println(str1 + "and" +str2 + "have the same setof character ");
}
else {
    System.out.println(str1 + "and" +str2 + "not the same set of character);
}
}
}